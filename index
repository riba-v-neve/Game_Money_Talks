    const EVENTS = RAW_EVENTS.filter(e => e.id !== "late_fee_debt");


    function makeNewGame(){
      return {
        day: 1,
        money: 7000,
        debt: 0,
        mood: 84,
        energy: 82,
        stress: 0,

        usedEventIds: [],
        frugalStreak: 0,
        isolationStreak: 0,

        currentEventId: null,
        resolvedToday: false,

        stats: {
          sumMood: 0,
          daysTracked: 0,
          maxDebt: 0,
          maxMoney: 7000,
          impulses: 0,
          minusCount: 0
        },

        flags: {
          itmo: true,
          dorm: true
        }
      };
    }

    let G = null;
    let lossReason = null;

    let pendingIncome = null;
    let afterIncomeContinue = null;
    let CURRENT_EVENT = null;

