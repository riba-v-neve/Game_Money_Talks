  <script>
"use strict";

    const DEBT_LIMIT = 15000;

    const clamp = (n, a, b) => Math.max(a, Math.min(b, n));

    function formatRub(n){
      const s = Math.round(n).toLocaleString("ru-RU");
      return s + " ₽";
    }

    function deepCopy(obj){
      return JSON.parse(JSON.stringify(obj));
    }

    function escapeHTML(str){
      return String(str)
        .replace(/&/g, "&amp;")
        .replace(/</g, "&lt;")
        .replace(/>/g, "&gt;")
        .replace(/"/g, "&quot;")
        .replace(/'/g, "&#039;");
    }

    // Лёгкая типографика: ёлочки и неразрывные пробелы после коротких предлогов
    function typograf(str){
      let s = String(str ?? "");
      s = s.replace(/"([^"]+)"/g, "«$1»");
      s = s.replace(/\s+/g, " "); // поджать лишние пробелы
      s = s.replace(/\b(в|к|с|у|о|об|на|по|за|от|до|из|без|для|под|над|при)\s+/gi, (m,p)=>p+"\u00A0");
      return s.trim();
    }

    /* Fixed calendar events */
