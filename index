    function showOnly(screen){
      [screenMenu, screenPrologue, screenGame, screenEnd].forEach(s => s.classList.add("hidden"));
      screen.classList.remove("hidden");
    }

    function openHow(){ modalHow.classList.remove("hidden"); }
    function closeHow(){ modalHow.classList.add("hidden"); }

    function openIncomeModal(sum, labels){
      pendingIncome = { sum, labels };
      const labelText = labels.length ? "\n" + labels.map(l => "• " + l).join("\n") : "";
      incomeText.textContent = typograf(
        "Пришёл доход: +" + formatRub(sum) + "." +
        (labelText ? " " : "") +
        (labelText ? "\n" + labels.map(l => "• " + l).join("\n") : "") +
        (G.debt > 0 ? ("\n\nСейчас у тебя есть долг: " + formatRub(G.debt) + ".\nХочешь направить этот доход на погашение?") : "")
      );
      modalIncome.classList.remove("hidden");
      btnIncomeDebt.classList.toggle("hidden", !(G.debt > 0));
    }

    function closeIncomeModal(){ 
      modalIncome.classList.add("hidden");
      pendingIncome = null;
    }

    function buildCalendar(){
      calendarEl.innerHTML = "";
      const totalCells = 35;
      const startOffset = 1;
      for(let i=0;i<totalCells;i++){
        const dayNum = i - startOffset + 1;
        const cell = document.createElement("div");
        cell.className = "calCell";
        if(dayNum < 1 || dayNum > 30){
          cell.classList.add("dim");
          cell.textContent = "";
        }else{
          cell.textContent = String(dayNum);
          const markers = document.createElement("div");
          markers.className = "markers";
          const dayEvents = SCHEDULE.filter(x => x.day === dayNum);
          dayEvents.forEach(x=>{
            const dot = document.createElement("span");
            dot.className = "mDot " + (x.type === "income" ? "income" : "bill");
            markers.appendChild(dot);
          });
          cell.appendChild(markers);
        }
        calendarEl.appendChild(cell);
      }
    }

    function paintCalendarCurrent(){
      const cells = Array.from(calendarEl.querySelectorAll(".calCell"));
      cells.forEach(c => c.classList.remove("current"));
      for(const c of cells){
        if(c.classList.contains("dim")) continue;
        const raw = (c.childNodes[0] && c.childNodes[0].nodeType === Node.TEXT_NODE) ? c.childNodes[0].nodeValue : c.textContent;
        const d = parseInt(raw, 10);
        if(d === G.day){
          c.classList.add("current");
        }
      }
    }

    function renderUpNext(){
      upNextEl.innerHTML = "";
      const next = SCHEDULE
        .filter(x => x.day >= G.day)
        .sort((a,b)=>a.day-b.day)
        .slice(0,2);

      if(next.length === 0){
        const div = document.createElement("div");
        div.className = "nextItem";
        div.innerHTML = "<b>НЕТ</b><span class='amt'>-</span>";
        upNextEl.appendChild(div);
        return;
      }

      next.forEach(x=>{
        const div = document.createElement("div");
        div.className = "nextItem";
        const amtClass = x.amount >= 0 ? "pos" : "neg";
        const amt = (x.amount >= 0 ? "+" : "") + formatRub(Math.abs(x.amount));
        div.innerHTML =
          "<b>ДЕНЬ " + x.day + "</b>" +
          "<span style='flex:1; text-align:left; opacity:.92;'>" + escapeHTML(typograf(x.label)) + "</span>" +
          "<span class='amt " + amtClass + "'>" + escapeHTML(amt) + "</span>";
        upNextEl.appendChild(div);
      });
    }

    function spend(amount){
      if(amount <= 0) return;
      if(G.money >= amount){
        G.money -= amount;
      }else{
        const deficit = amount - G.money;
        G.money = 0;
        G.debt += deficit;
        G.stats.minusCount += 1;
        G.stats.maxDebt = Math.max(G.stats.maxDebt, G.debt);
      }
    }

    function dailyBaseCosts(){
      // Минимальные ежедневные траты на еду (без проезда - он отдельным платежом)
      const food = 210;
      spend(food);

      // Восстановление после сна: базово окей, но стресс и долги подрезают ресурс
      const st = (typeof G.stress === "number") ? G.stress : 0;

      // базово: энергия +3, настроение без плюса (если всё спокойно)
      let de = 3 - Math.ceil(st * 0.7);
      let dm = 0 - Math.floor(st / 2);

      // если денег почти нет - тревожнее и хуже спишь
      if(G.money < 700){ dm -= 1; de -= 1; }

      // долг давит сильнее, чем кажется
      if(G.debt > 0) dm -= 2;

      // когда совсем выжат - организм пытается добрать сон
      if(G.energy < 20) de += 2;

      G.mood = clamp(G.mood + dm, 0, 100);
      G.energy = clamp(G.energy + de, 0, 100);
    }

    function applySchedule(){
      const today = SCHEDULE
        .filter(x=>x.day===G.day)
        .sort((a,b)=> (a.type === b.type) ? 0 : (a.type === "income" ? -1 : 1));

      let incomeSum = 0;
      const incomeLabels = [];

      for(const ev of today){
        if(ev.amount >= 0){
          incomeSum += ev.amount;
          incomeLabels.push(ev.label + " (" + "+" + formatRub(ev.amount) + ")");
          G.money += ev.amount;
          G.stats.maxMoney = Math.max(G.stats.maxMoney, G.money);
        }else{
          spend(-ev.amount);
        }
      }

      return { incomeSum, incomeLabels };
    }

    function normalizeAndClamp(){
      if(G.money < 0){
        G.debt += Math.abs(G.money);
        G.money = 0;
        G.stats.minusCount += 1;
      }
      if(G.debt < 0) G.debt = 0;
      G.mood = clamp(G.mood, 0, 100);
      G.energy = clamp(G.energy, 0, 100);

      G.stats.maxMoney = Math.max(G.stats.maxMoney, G.money);
      G.stats.maxDebt = Math.max(G.stats.maxDebt, G.debt);
    }

    function checkLose(){
      if(G.mood <= 0){
        lossReason = "Настроение упало до нуля. Ты выгорел(а) и просто не смог(ла) продолжать месяц.";
        return true;
      }
      if(G.energy <= 0){
        lossReason = "Энергия упала до нуля. Организм взял паузу, а месяц не подождал.";
        return true;
      }
      if(G.debt >= DEBT_LIMIT){
        lossReason = "Долг дошёл до лимита " + formatRub(DEBT_LIMIT) + ".";
        return true;
      }
      return false;
    }

    function eventMeetsConditions(ev){
      const c = ev.conditions || {};
      if(typeof c.minMoney === "number" && G.money < c.minMoney) return false;
      if(typeof c.maxMoney === "number" && G.money > c.maxMoney) return false;
      if(typeof c.minDebt === "number" && G.debt < c.minDebt) return false;
      if(typeof c.maxDebt === "number" && G.debt > c.maxDebt) return false;
      if(typeof c.minMood === "number" && G.mood < c.minMood) return false;
      if(typeof c.maxMood === "number" && G.mood > c.maxMood) return false;
      if(typeof c.minEnergy === "number" && G.energy < c.minEnergy) return false;
      if(typeof c.maxEnergy === "number" && G.energy > c.maxEnergy) return false;
      if(typeof c.minIsolationStreak === "number" && G.isolationStreak < c.minIsolationStreak) return false;
      return true;
    }

    function weightFor(ev){
      let w = 1;

      if(G.energy <= 45 && (ev.cat === "риск" || ev.cat === "психика")) w += 1.2;
      if(G.mood <= 45 && ev.cat === "психика") w += 1.8;
      if(G.money <= 900 && ev.cat === "возможность") w += 1.2;
      if(G.debt >= 1200 && (ev.cat === "долг" || ev.cat === "финансы")) w += 1.6;

      if(G.isolationStreak >= 2 && ev.id === "apathy_burnout") w += 6;
      if(G.frugalStreak >= 2 && ev.cat === "возможность") w *= 0.65;
return Math.max(0.01, w);
    }

    function makeFallbackEvent(){
      const id = "quiet_day_" + G.day;
      const titles = ["Тихий день", "Окно между парами", "Без сюрпризов"];
      const title = titles[(G.day * 7) % titles.length];

      return {
        id,
        cat:"рутина",
        title,
        text:`Сегодня без крупных сюрпризов. Можно восстановиться или сделать спокойный рывок по учёбе.`,
        choices:[
          { key:"A", label:"Отдохнуть и лечь пораньше", effects:{ money:0, mood:+1, energy:+6 }, flags:["balanced"] },
          { key:"B", label:"Рывок по учёбе", effects:{ money:0, mood:+2, energy:-3 }, flags:["frugal"] }
        ],
        lesson:"Стабильный режим иногда полезнее, чем героизм."
      };
    }

    function pickEvent(){
      if(!G.usedEventIds) G.usedEventIds = [];

      if(G.isolationStreak >= 3){
        const forced = EVENTS.find(e => e.id === "apathy_burnout");
        if(forced && eventMeetsConditions(forced) && !G.usedEventIds.includes(forced.id)) return forced;
      }

      const pool = EVENTS.filter(ev => eventMeetsConditions(ev) && !G.usedEventIds.includes(ev.id));
      if(pool.length === 0) return makeFallbackEvent();

      const weights = pool.map(weightFor);
      const sum = weights.reduce((a,b)=>a+b,0);

      let r = Math.random() * sum;
      for(let i=0;i<pool.length;i++){
        r -= weights[i];
        if(r <= 0) return pool[i];
      }
      return pool[pool.length-1];
    }

    function renderHUD(){
      hudDay.textContent = G.day + "/30";
      hudMoney.textContent = formatRub(G.money);
      hudDebt.textContent = formatRub(G.debt);
      hudDebtLimit.textContent = formatRub(DEBT_LIMIT);

      hudMoodVal.textContent = String(G.mood);
      hudEnergyVal.textContent = String(G.energy);

      barMood.style.width = clamp(G.mood,0,100) + "%";
      barEnergy.style.width = clamp(G.energy,0,100) + "%";

      const ratio = clamp(G.debt / DEBT_LIMIT, 0, 1);
      barDebt.style.width = (ratio * 100) + "%";
      const hue = Math.round(120 * (1 - ratio)); // 120=green -> 0=red
      barDebt.style.background = "linear-gradient(90deg, hsl(" + hue + ", 90%, 45%), rgba(255,255,255,0.25))";

      hudDebt.classList.toggle("warn", G.debt > 0);
      hudDebt.classList.toggle("ok", G.debt === 0);

      hudFlags.textContent = "Студент ИТМО, 3 курс";

      debtActions.classList.toggle("hidden", !(G.debt > 0));
      // Кнопки погашения - только если хватает денег
      btnPay500.disabled = !(G.money >= 500 && G.debt > 0);
      btnPay1000.disabled = !(G.money >= 1000 && G.debt > 0);
      btnPayAll.disabled = !(G.money > 0 && G.debt > 0);

      paintCalendarCurrent();
      renderUpNext();
    }

    function setChoicesEnabled(on){
      [choiceA, choiceB, choiceC].forEach(b => b.disabled = !on);
    }

    function renderEvent(ev){
      CURRENT_EVENT = ev;
      G.currentEventId = ev.id;
      markEventUsed(ev.id);
      G.resolvedToday = false;

      eventTitle.textContent = typograf(ev.title);
      eventText.textContent = typograf(ev.text);
      eventCat.textContent = String(ev.cat || "ситуация").toUpperCase();
      eventMeta.textContent = "День " + G.day + ", Санкт-Петербург";

      choiceA.textContent = "A. " + typograf(ev.choices[0].label);
      choiceB.textContent = "B. " + typograf(ev.choices[1].label);

      choiceC.classList.add("hidden");
      if(ev.choices[2]){
        choiceC.textContent = typograf(ev.choices[2].label);
        choiceC.classList.remove("hidden");
      }

      resultPanel.classList.add("hidden");
      deltaRow.innerHTML = "";
      lessonEl.textContent = "";

      setChoicesEnabled(true);
    }

    function markEventUsed(id){
      if(!id) return;
      if(!G.usedEventIds) G.usedEventIds = [];
      if(!G.usedEventIds.includes(id)) G.usedEventIds.push(id);
    }

    function renderResult(d){
      deltaRow.innerHTML = "";

      function addDelta(label, value, kind){
        if(value === 0) return;
        const div = document.createElement("div");
        div.className = "delta " + (value > 0 ? "pos" : "neg");
        div.innerHTML = "<span>" + escapeHTML(label) + "</span><span class='v'>" +
          (value > 0 ? "+" : "-") + (kind === "rub" ? escapeHTML(formatRub(Math.abs(value))) : escapeHTML(String(Math.abs(value)))) +
          "</span>";
        deltaRow.appendChild(div);
      }

      addDelta("ДЕНЬГИ", d.money, "rub");
      addDelta("ДОЛГ", d.debt, "rub");
      addDelta("НАСТРОЕНИЕ", d.mood, "num");
      addDelta("ЭНЕРГИЯ", d.energy, "num");

      if(deltaRow.children.length === 0){
        const div = document.createElement("div");
        div.className = "delta";
        div.innerHTML = "<span>ИЗМЕНЕНИЙ НЕТ</span><span class='v'>0</span>";
        deltaRow.appendChild(div);
      }
    }

    function applyChoice(choice){
      if(G.resolvedToday) return;

      const before = { money:G.money, debt:G.debt, mood:G.mood, energy:G.energy };

      let eff = choice.effects;
      let delta = {};
      if(typeof eff === "function"){
        delta = eff(G) || {};
      }else{
        delta = deepCopy(eff || {});
      }

      if(typeof delta.money === "number") G.money += delta.money;
      if(typeof delta.debt === "number") G.debt += delta.debt;
      if(typeof delta.mood === "number") G.mood += delta.mood;
      if(typeof delta.energy === "number") G.energy += delta.energy;

      normalizeAndClamp();

      const flags = choice.flags || [];

      // Стресс: копится от импульсивных/тяжёлых дней и влияет на восстановление
      if(typeof G.stress !== "number") G.stress = 0;
      let ds = 0;
      if(flags.includes("impulse")) ds += 1;
      if(flags.includes("work")) ds += 1;
      if(flags.includes("isolate")) ds += 1;
      if(flags.includes("social")) ds -= 1;
      if(flags.includes("frugal") || flags.includes("balanced")) ds -= 1;
      if(ds !== 0) G.stress = clamp(G.stress + ds, 0, 6);

      if(flags.includes("impulse")) G.stats.impulses += 1;

      if(flags.includes("frugal")) G.frugalStreak += 1;
      else G.frugalStreak = 0;

      if(flags.includes("isolate")) G.isolationStreak += 1;
      else{
        if(flags.includes("social") || flags.includes("work") || flags.includes("balanced") || flags.includes("impulse")){
          G.isolationStreak = 0;
        }
      }

      const after = { money:G.money, debt:G.debt, mood:G.mood, energy:G.energy };
      const real = {
        money: after.money - before.money,
        debt: after.debt - before.debt,
        mood: after.mood - before.mood,
        energy: after.energy - before.energy
      };

      renderResult(real);

      const ev = CURRENT_EVENT;
      lessonEl.innerHTML = "<b>Урок дня:</b> " + escapeHTML(typograf(ev?.lesson || "Фиксируй доходы и обязательные траты - так легче держать картину в голове."));

      G.resolvedToday = true;
      setChoicesEnabled(false);
      resultPanel.classList.remove("hidden");

      if(checkLose()){
        endGame(false);
      }else{
        renderHUD();
      }
    }

    function continueStartDay(){
      if(checkLose()){ endGame(false); return; }
      const ev = pickEvent();
      renderHUD();
      renderEvent(ev);
    }

    function startDay(){
      lossReason = null;

      dailyBaseCosts();
      const incomeInfo = applySchedule();
      normalizeAndClamp();

      if(checkLose()){ endGame(false); return; }

      // Если пришёл доход и есть долг - предлагаем сразу распределить
      if(incomeInfo.incomeSum > 0 && G.debt > 0){
        afterIncomeContinue = continueStartDay;
        renderHUD();
        openIncomeModal(incomeInfo.incomeSum, incomeInfo.incomeLabels);
        return;
      }

      continueStartDay();
    }

    
    function eveningFatigue(){
      const st = (typeof G.stress === "number") ? G.stress : 0;

      // К концу дня ресурс уходит почти всегда
      let de = -4 - Math.floor(st / 3);
      let dm = -2 - Math.floor(st / 3);

      // когда день прошёл на последних процентах - морально тяжелее
      if(G.energy < 25) dm -= 1;

      G.energy = clamp(G.energy + de, 0, 100);
      G.mood = clamp(G.mood + dm, 0, 100);
    }

function endDayAndAdvance(){
      if(!G.resolvedToday) return;

      // Вечерний расход ресурса - чтобы игра была про баланс, а не «вечно 100%»
      eveningFatigue();
      normalizeAndClamp();

      if(checkLose()){
        endGame(false);
        return;
      }

      G.stats.sumMood += G.mood;
      G.stats.daysTracked += 1;

      if(G.day >= 30){
        endGame(true);
        return;
      }

      G.day += 1;
      startDay();
    }

    function endGame(win){
      showOnly(screenEnd);

      const avgMood = G.stats.daysTracked > 0 ? Math.round(G.stats.sumMood / G.stats.daysTracked) : G.mood;

      if(win){
        endTitle.textContent = "ПОБЕДА";
        endReason.textContent = typograf("Ты прожил(а) 30 дней. Баланс денег, долга и состояния сработал.");
      }else{
        endTitle.textContent = "ПОРАЖЕНИЕ";
        endReason.textContent = typograf(lossReason || "Система не выдержала: деньги и ресурс закончились.");
      }

      const rows = [
        ["ФИНАЛЬНЫЕ ДЕНЬГИ", formatRub(G.money)],
        ["ФИНАЛЬНЫЙ ДОЛГ", formatRub(G.debt)],
        ["СРЕДНЕЕ НАСТРОЕНИЕ", String(avgMood)],
        ["МАКС. ДОЛГ", formatRub(G.stats.maxDebt)],
        ["МАКС. ДЕНЕГ НА БАЛАНСЕ", formatRub(G.stats.maxMoney)],
        ["ИМПУЛЬСНЫЕ ПОКУПКИ", String(G.stats.impulses)],
        ["УХОДИЛ(А) В ДОЛГ", String(G.stats.minusCount)]
      ];

      statsGrid.innerHTML = "";
      rows.forEach(([k,v])=>{
        const div = document.createElement("div");
        div.className = "stat";
        div.innerHTML = "<span>" + escapeHTML(typograf(k)) + "</span><b>" + escapeHTML(typograf(v)) + "</b>";
        statsGrid.appendChild(div);
      });
    }

    function beginNew(){
      G = makeNewGame();
      lossReason = null;
      showOnly(screenPrologue);
    }

    function startFromPrologue(){
      showOnly(screenGame);
      buildCalendar();
      renderHUD();
      startDay();
    }

    function payDebt(amount){
      if(!G || G.debt <= 0) return;
      const pay = Math.min(G.debt, G.money, amount === Infinity ? G.money : amount);
      if(pay <= 0) return;

      G.money -= pay;
      G.debt -= pay;

      normalizeAndClamp();
      renderHUD();

      if(checkLose()) endGame(false);
    }

    // --- UI events
    btnHow.addEventListener("click", openHow);
    btnCloseHow.addEventListener("click", closeHow);
modalHow.addEventListener("click", (e)=>{ if(e.target === modalHow) closeHow(); });

    btnNew.addEventListener("click", beginNew);
    btnBackMenu1.addEventListener("click", ()=>{ showOnly(screenMenu); });
    btnStartGame.addEventListener("click", startFromPrologue);

    btnNextDay.addEventListener("click", endDayAndAdvance);
    btnExitToMenu.addEventListener("click", ()=>{ closeHow(); closeIncomeModal(); showOnly(screenMenu); });

    btnBackMenu2.addEventListener("click", ()=>{ showOnly(screenMenu); });
    btnRestart.addEventListener("click", ()=>{ G = makeNewGame(); lossReason = null; showOnly(screenPrologue); });

    btnPay500.addEventListener("click", ()=> payDebt(500));
    btnPay1000.addEventListener("click", ()=> payDebt(1000));
    btnPayAll.addEventListener("click", ()=> payDebt(Infinity));

    function bindChoice(button, index){
      button.addEventListener("click", ()=>{
        const ev = CURRENT_EVENT;
        if(!ev) return;
        const ch = ev.choices[index];
        if(!ch) return;
        applyChoice(ch);
      });
    }
    bindChoice(choiceA, 0);
    bindChoice(choiceB, 1);

    choiceC.addEventListener("click", ()=>{
      const ev = EVENTS.find(e=>e.id===G.currentEventId);
      if(!ev || !ev.choices[2]) return;
      applyChoice(ev.choices[2]);
    });

    // Avatar dance (click)
    function playDance(){
      if(!avatarInner) return;
      avatarInner.classList.remove("dance");
      // reflow to restart animation
      void avatarInner.offsetWidth;
      avatarInner.classList.add("dance");
    }
    hudAvatar.addEventListener("click", playDance);
    hudAvatar.addEventListener("keydown", (e)=>{ if(e.key === "Enter" || e.key === " ") playDance(); });

    // Income modal actions
    function finishIncomeFlow(){
      closeIncomeModal();
      renderHUD();
      if(typeof afterIncomeContinue === "function"){
        const f = afterIncomeContinue;
        afterIncomeContinue = null;
        f();
      }
    }

    btnIncomeBalance.addEventListener("click", ()=>{
      finishIncomeFlow();
    });

    btnIncomeDebt.addEventListener("click", ()=>{
      if(!pendingIncome) return finishIncomeFlow();
      const cap = pendingIncome.sum || 0;
      const pay = Math.min(cap, G.debt, G.money);
      if(pay > 0){
        G.money -= pay;
        G.debt -= pay;
        normalizeAndClamp();
      }
      finishIncomeFlow();
    });

    modalIncome.addEventListener("click", (e)=>{ 
      if(e.target === modalIncome) finishIncomeFlow();
    });

    window.addEventListener("keydown", (e)=>{
      if(modalHow && !modalHow.classList.contains("hidden")){
        if((e.key || "").toLowerCase() === "escape") closeHow();
        return;
      }
      if(modalIncome && !modalIncome.classList.contains("hidden")){
        if((e.key || "").toLowerCase() === "escape") finishIncomeFlow();
        return;
      }

      if(screenGame.classList.contains("hidden")) return;

      const k = (e.key || "").toLowerCase();
      if(k === "a"){
        if(!choiceA.disabled) choiceA.click();
      }else if(k === "b"){
        if(!choiceB.disabled) choiceB.click();
      }else if(k === "enter"){
        if(!btnNextDay.disabled && !resultPanel.classList.contains("hidden")) btnNextDay.click();
      }else if(k === "escape"){
        // no-op
      }
    });

    (function boot(){
      buildCalendar();
      showOnly(screenMenu);
    })();
  </script>
</body>
</html>
